<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col"><%= Signature.all.count %> Unterschriften</th>
      </tr>
    </thead>
    <tbody>
      <% Signature.last(10).reverse.each do |signature| %>
      <tr>
        <td>
          <%= "#{signature.firstname}  #{signature.lastname}" %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
